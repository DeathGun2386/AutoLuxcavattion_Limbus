#name "Mod_Energy"            

//___Сбор энергии___
SUB(Energy, $xEx, $yEx, $SxEx, $SyEx)
   LCLICK(ROUND(($SxEx*0,3048),0)+$xEx,ROUND(($SyEx*0,9129),0)+$yEx)
	 WAIT(1)
	 LCLICK(ROUND(($SxEx*0,6295),0)+$xEx,ROUND(($SyEx*0,4569),0)+$yEx)
	 WAIT(1)
	 KEYPRESS(#ENTER)
	 WAIT(3)
	 KEYPRESS(#ESC)
	 WAIT(5)
END_SUB